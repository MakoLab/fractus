	<searchParams>
		<query/>
		<columns>
			<column field="identifier" sortOrder="1" sortType="ASC"/>
			<column field="shortName" />
			<column field="description" />
		</columns>
		<filters/> 
	</searchParams>
	<operations>
		<item name="newItem" global="1">Stwórz nowy przedmiot...</item>
