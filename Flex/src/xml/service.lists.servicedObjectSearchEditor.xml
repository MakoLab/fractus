	<columns>
		<column field="@identifier" labelKey="service.servicedObjectIdentifier" width="150" filter="1"/>
		<column field="@shortName" labelKey="contractors.contractor" width="200" filter="1"/>
		<column field="@description" labelKey="service.servicedObjectDescription" filter="1"/>
	</columns>
</config>