<?xml version="1.0" encoding="utf-8"?>
<root>
  <commercialDocument>
    <documentTypeId/>
    <contractor>
      <contractor>...</contractor>
      <addressId/>
    </contractor>
    <receivingPerson>
      <contractor>...</contractor>
    </receivingPerson>
    <issuer>
      <contractor>...</contractor>
      <addressId/>
    </issuer>
    <issuingPerson>
      <contractor>...</contractor>
    </issuingPerson>
    <documentCurrencyId/>
    <systemCurrencyId/> <!-- uzupelnia kernel -->
    <exchangeDate/>
    <exchangeScale/>
    <exchangeRate/>
    <status/>
    <number>
      <number/>
      <fullNumber/>
      <numberSettingId/>
    </number>
    <issuePlaceId/>
    <issueDate/>
    <eventDate/>
    <netValue/>
    <grossValue/>
    <vatValue/>
    <printDate/>
    <isExportedForAccounting/>
    <netCalculationType/>
    <vatRatesSummationType/>
    
    <lines>
      <line>
        <ordinalNumber/>
        <itemId/>
        <itemVersion/>
        <itemName/>
        <quantity/>
        <netPrice/>
        <grossPrice/>
        <initialNetPrice/>
        <initialGrossPrice/>
        <discountRate/>
        <discountNetValue/>
        <discountGrossValue/>
        <initialNetValue/>
        <initialGrossValue/>
        <netValue/>
        <grossValue/>
        <vatValue/>
        <vatRateId/>
        <unitId/>
        <commercialDirection/>
        <orderDirection/>

        <commercialWarehouseValuations>
          <commercialWarehouseValuation>
            <relatedLine>
              <line>
                <id/>
              </line>
            </relatedLine>
            <quantity/>
            <value/>
            <price/>
          </commercialWarehouseValuation>
          <!--Multiple-->
        </commercialWarehouseValuations>

        <commercialWarehouseRelations>
          <commercialWarehouseRelation>
            <relatedLine>
              <line>
                <id/>
              </line>
            </relatedLine>
            <quantity/>
            <value/>
            <isValuated/>
          </commercialWarehouseRelation>
          <!--Multiple-->
        </commercialWarehouseRelations>
      </line>
      <!--Multiple-->
    </lines>

    <payments>
      <payment>
        <date/>
        <dueDate/>
        <contractor>
          <contractor>...</contractor>
          <addressId/>
        </contractor>
        <paymentMethodId/>
        <amount/>
        <paymentCurrencyId/>
        <systemCurrencyId/> <!-- uzupelnia kernel -->
        <exchangeDate/>
        <exchangeScale/>
        <exchangeRate/>
        <isSettled/>
        <description/><!--NULLABLE-->
      </payment>
      <!--Multiple-->
    </payments>

    <attributes>
      <attribute>
        <documentFieldId/>
        <value/>
      </attribute>
      <!--Multiple-->
    </attributes>

    <vatTable>
      <vtEntry>
        <vatRateId/>
        <netValue/>
        <grossValue/>
        <vatValue/>
      </vtEntry>
      <!--Multiple-->
    </vatTable>
  </commercialDocument>
</root>